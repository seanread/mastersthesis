@Article{Chouly2009,
  Title                    = {Modelling the human pharyngeal airway: validation of numerical simulations using in vitro experiments},
  Author                   = {Chouly, Franz and Van Hirtum, Annemie and LagrÃ©e, Pierre-Yves and Pelorson, Xavier and Payan, Yohan},
