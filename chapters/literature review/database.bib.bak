@Article{Rombaux,
  Title                    = {Assessment ofÂ olfactory andÂ trigeminal function using chemosensory event-related potentials},
  Author                   = {Rombaux, P. and Mouraux, A. and Bertrand, B. and Guerit, J. M. and Hummel, T.},
