@Article{Durbin2009,
  Title                    = {Limiters and wall treatments in applied turbulence modeling},
  Author                   = {Durbin, P. A.},
  Journal                  = {Fluid Dynamics Research},
  Year                     = {2009},
  Number                   = {1},
