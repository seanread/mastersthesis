@Article{GroAYe2007,
  Title                    = {Time resolved analysis of steady and oscillating flow in the upper human airways},
  Author                   = {GroÃŸe, S. and SchrÃ¶der, W. and Klaas, M. and KlÃ¶ckner, A. and Roggenkamp, J.},
  Journal                  = {Experiments in Fluids},
